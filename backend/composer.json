{
  "name": "fracz/supla-scripts",
  "type": "project",
  "require": {
    "ext-curl": "*",
    "ext-json": "*",
    "beberlei/assert": "^2.7",
    "defuse/php-encryption": "^2.0",
    "firebase/php-jwt": "^4.0",
    "illuminate/database": "^5.3",
    "illuminate/events": "^5.3",
    "illuminate/pagination": "^5.3",
    "jdorn/file-system-cache": "@dev",
    "m6web/statsd": "^1.3",
    "monolog/monolog": "^1.23",
    "mtdowling/cron-expression": "^1.2",
    "ramsey/uuid": "^3.7",
    "robmorgan/phinx": "^0.8",
    "tuupola/slim-basic-auth": "^2.2",
    "tuupola/slim-jwt-auth": "^2.3",
    "slim/slim": "^3.8",
    "supla/api-client": "dev-master",
    "symfony/console": "^3.3",
    "symfony/process": "^3.3",
    "twig/twig": "^2.5"
  },
  "autoload": {
    "psr-4": {
      "suplascripts\\": "./"
    }
  },
  "scripts": {
    "phpcs": "phpcs --ignore=*/vendor/*,*/database/migrations/* --standard=../scripts/phpcs-rules.xml .",
    "phpcbf": "phpcbf --ignore=*/vendor/*,*/database/migrations/* --standard=../scripts/phpcs-rules.xml ."
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.1"
  }
}
